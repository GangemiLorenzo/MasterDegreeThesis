{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Editor",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/editor/lib/main.dart"
    },
    {
      "name": "Launch Codec",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/codec/codec.go"
    },
    {
      "name": "Launch Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/server/server.go"
    },
    {
      "name": "Launch Auditor",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/auditor/auditor.go"
    },
    {
      "name": "Launch Assistant",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/ai_assistant/assistant.go"
    }
  ],
  "compounds": [
    {
      "name": "Launch services",
      "configurations": [
        "Launch Codec",
        "Launch Auditor",
        "Launch Server",
        "Launch Assistant"
      ],
      "stopAll": true
    }
  ],
}